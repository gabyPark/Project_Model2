<script type="text/javascript">
$(function() {
	loadList(1);
	
	var $wrap = $('.cn_wrap');

	/*!
		베스트 리뷰
	*/
	var options = {};
	options.draggable = false;
	options.speed = 1200;
	options.variableWidth = true;
	options.waitForAnimate = false;
	options.events = { carouselinit:initCarousels, carouselafterchange:afterCarousels }; 
	$wrap.find('.ui_carousel').vcCarousel( options );

	function initCarousels( evt, carousel ) { 
		$(carousel.el).find('.count_wrap>span.num').html('<em>1</em>/'+carousel.$slides.length); 
	} 

	function afterCarousels( evt, carousel, index ) { 
		var template = '<em>'+(carousel.currentSlide+1)+'</em>/'+carousel.$slides.length; 
		$(carousel.el).find('.count_wrap>span.num').html( template ); 
	} 

	/*!
		리뷰 댓글
		- 리뷰 모듈 썸네일, 펼쳐보기
		- 리뷰 모듈 썸네일, 이미지 총 개수 표기
	*/
	var $tasteReviewList = $wrap.find('.taste_review_list');
	$tasteReviewList.on( 'click', 'a.link_more', function() {
		var $reviewModule = $(this).closest('.review_module');
		$reviewModule.toggleClass('open');
		$reviewModule.find('https://www.cjcookit.com/pc/rvw/p.txt').toggleClass('mt_elps');
		return false;
	});

	setReviewThumbImageLen( $tasteReviewList );

	function setReviewThumbImageLen( $listWrap ) {
		var $tasteReviewLists = $listWrap.find('>ul>li');
		$.each( $tasteReviewLists, function( i, item ) {
			if( !!!$(item).find('.img_wrap>div.img').length ) return;
			var imageLen = $(item).find('.img_wrap>div.img').length;
			imageLen = '<span class="hide">총 사진 개수</span>'+imageLen;
			$(item).find('a.link_more span.lang').html(imageLen);
		});
	}
	
// 	$('input[name=sort]').on('change', function(i, e) {
// 		loadList(1);
// 	});
	
	
});

$(document).ready(function(){
	$.each($("img[name^=imgResizingCookit]"), function(index, item){
		var imgObject = {};
		imgObject.url = $(this).attr("url");
		imgObject.w = 0;
		imgObject.h = 0;
		imgObject.obj = this;
		image.init(imgObject);
	});
});

function goPage(page) {
	$(window).scrollTop($(".top_sec").offset().top);
	loadList(page);
}

function getSelectedOpt() {
	
	var opt = {
			keyWord	: $("input[name=searchText]").val(),
// 			order	  : $("input[name=sort]:checked").val(),
			order	  : '01',
			photoRvwYn : $("#sorting_photo").prop("checked") ? "Y" : "N"
	};
	
	return opt;
}

function loadList(page) {
	if (cj.isEmpty(page)) { return false; }
	
	//if (page == 1) { $(".reviewArea").empty(); }
	
	var opt = $.extend(true, {page : page}, getSelectedOpt());
	
	if(!cj.isEmpty($("input[name=searchText]").val())) {
		$("#fn_txt_srch").val($("input[name=searchText]").val());
	}
	// 전송
	$.ajax({
		async		: true,
		url			: '/pc/rvw/reviewList.json',
		dataType	: 'json',
		type		: 'POST',
		data		: opt,
		success	: function(data, textStatus, jqXHR){
			if(!crew.ajaxValidate(data)){ return; }
			
			// html 적용
			$(".reviewArea").html(data.html);
			
			// paging
			$('#paging').html(data.paging);
					
			// 페이지 정보 세팅
			setPageInfo(data);
			
			if(data.totalCount < 1) {
				$('.no_data').show();
			} else {
				$('.no_data').hide();
			}
		},
		error	  : function(jqXHR, textStatus, errorThrown){
		  //console.log(jqXHR.status);
		}
	});
}


//페이지 정보 세팅
function setPageInfo(data) {
	if (data.totalCount != 0) {
		var searchText = $("#fn_txt_srch").val();
		if (!cj.isEmpty(searchText)) {
			$(".reviewArea li").each(function(){
			
				var title = $(this).find("p.tit > span.name").text();
				
				title = title.split(searchText).join("<strong>"+searchText+"</strong>");
				$(this).find("p.tit > span.name").html(title);
			})
		}
		$.each($("img[name^=imgResizingCookitSub]"), function(index, item){
			var imgObject = {};
			imgObject.url = $(this).attr("url");
			imgObject.w = 0;
			imgObject.h = 0;
			imgObject.obj = this;
			image.init(imgObject);
		});

	}
	//리뷰 <span class="rvwTotalCnt">0</span>건
	//$('.rvwTotalCnt').text(numberFormat(data.totalCount));
	$('.total').html("리뷰 <span class='rvwTotalCnt'>" + numberFormat(data.totalCount) + "</span>건  ");
}

function numberFormat(number) {
    if(number==0) return 0;
    
    var reg = /(^[+-]?\d+)(\d{3})/;
    var n = (number + '');
 
    while (reg.test(n)) n = n.replace(reg, '$1' + ',' + '$2');
 
    return n;
}

// 검색 클릭시
function search() {
	$("#fn_txt_srch").val($.trim($("#fn_txt_srch").val()));
	
	var searchText = $("#fn_txt_srch").val();
	
	$("input[name=searchText]").val(searchText);
	loadList(1);
	
	location.href='#reviewTop';
}

// 상세보기 팝업
function openDetail(prdRvwSeq) {
	if ( cj.isEmpty(prdRvwSeq)) { return false;}
	
	cj.prodReview.openDetail(prdRvwSeq, '01');
}
</script>


